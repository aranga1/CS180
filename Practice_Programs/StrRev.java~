public class StrRev {
    public String revString(String str) {
        String temp = "";
        for (int i = 0; i < str.length(); i++) {
            if (str.charAt(i) == 32) {
                temp = " " + temp;
            }
            else {
                temp = str.charAt(i) + temp;
            }
        }
        return temp;
    }
    
    public static void main(String args[]) {
        StrRev s = new StrRev();
        String n = "This is me";
        System.out.println(s.revString(n));
    }
    